{
  "presets": [
    "@babel/env",
    "@babel/react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["module-resolver", {
      "root": ["./javascript"],
      "alias": {
        "Image": "./javascript/App/Image",
        "Style": "./javascript/App/Style"
      }
    }]
  ]
}